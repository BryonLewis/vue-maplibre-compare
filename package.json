    "@vitejs/plugin-vue": "^6.0.2",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.24",
    "vue-tsc": "^2.2.12"
